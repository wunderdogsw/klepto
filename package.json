{
	"name": "klepto",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"start": "npm run dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@material/typography": "^13.0.0",
		"@playwright/test": "^1.22.2",
		"@smui-extra/accordion": "^6.0.0-beta.16",
		"@smui-extra/badge": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/checkbox": "^6.0.0-beta.16",
		"@smui/dialog": "^6.0.0",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/form-field": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/layout-grid": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@smui/snackbar": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/sanitize-html": "^2.6.2",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^3.2.5"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "^2.28.0",
		"dotenv": "^16.0.1",
		"jose": "^4.8.3",
		"mongodb": "^4.7.0",
		"svelte-material-ui": "^6.0.0-beta.16"
	}
}
